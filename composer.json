{
    "name": "olps/omnipay-cardpointe",
    "type": "library",
    "description": "CardPointe gateway for Omnipay payment processing library",
    "keywords": [
        "gateway",
        "cardpointe",
        "fiserv",
        "omnipay",
        "pay",
        "payment"
    ],
    "homepage": "https://github.com/online-postal-scanning/omnipay-cardpointe",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Omnipay\\CardPointe\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "require": {
        "php": "^8.3",
        "omnipay/common": "^3.0"
    },
    "require-dev": {
        "omnipay/tests": "dev-master",
        "phpunit/phpunit": "^11.0",
        "squizlabs/php_codesniffer": "^3.0"
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": false
        }
    }
}